package com.elenilson.cursojavaintermediario.aula73;

public class MinhaThread implements Runnable{
	
	private String nome;
	
	
	public MinhaThread(String nome) {
		this.nome = nome;
		new Thread(this, nome).start();
	}

	@Override
	public void run() {
		
		System.out.println("Executando " + this.nome);
		
		for(int  i = 0; i < 10; i++) {
			
			Thread.sleep(300);
		}
		
	}

}
