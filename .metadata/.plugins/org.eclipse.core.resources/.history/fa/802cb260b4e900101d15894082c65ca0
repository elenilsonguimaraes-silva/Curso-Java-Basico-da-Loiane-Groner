package com.elenilson.cursojavaintermediario.aula67;

public class MinhaThread extends Thread {
	
	private String nome;
	private int tempo;
	
	public MinhaThread(String nome, int tempo) {
		this.nome = nome;
		this.tempo = tempo;
		start();
	}
	
	public void run() {
		try {
			
			for(int i = 0; i < 6; i++) {
				System.out.println(nome + " contador" + i);
				Thread.sleep(10000);
				
			}
			
		} catch (Exception e) {
			e.printStackTrace();
		}
		
	}

}
