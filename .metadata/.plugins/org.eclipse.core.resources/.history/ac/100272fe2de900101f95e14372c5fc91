package com.elenilson.cursojavaintermediario.aula65;

import com.elenilson.cursojavaintermediario.aula61.Contato;

public class ColetorLixo {
	
	public static void obterMemoriaUsada() {
		
		finally int MB = 1024 * 1024;
		
		Runtime runtime = Runtime.getRuntime(); //singleton
		
		runtime.totalMemory() - runtime.freeMemory();
	}

	public static void main(String[] args) {
		
		Contato[] contatos = new Contato[1000];
		Contato contato;
		
		for(int i = 0; i < contatos.length; i++) {
			contato = new Contato("Contato" + i, "1234-56789"+ i, "contato"+i + "@email.com");
			contatos[i] = contato;
		}
		
		System.out.println("Contatos criados");
		
		contatos = null;
		
		System.out.println("Contatos removidos da memÃ³ria");

	}

}
